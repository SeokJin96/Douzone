class Unit2{
	int hitpoint;
	final int MAX_HP;
	Unit2(int hp){
		this.MAX_HP = hp;
	}
}
//인터페이스
//~할 수 있는 (수리할 수 있는)
interface irepairable{}

//지상유닛, 공중유닛, 건물
class GroundUnit extends Unit2{
	GroundUnit(int hp) {
		super(hp);
	}
}

//공중유닛
class AirUnit extends Unit2{

	AirUnit(int hp) {
		super(hp);
	}
	
}

//건물
class CommandCenter {}


//유닛
class Tank2 extends GroundUnit{

	Tank2(int hp) {
		super(50);
		this.hitpoint = this.MAX_HP;
	}
	@Override
	public String toString() {
		return "Tank2";
	}
	
}

class Marine2 extends Unit2{

	Marine2(int hp) {
		super(50);
		this.hitpoint = this.MAX_HP;
	}
	
	@Override
	public String toString() {
		return "Tank2";
	}
	
}

class Scv extends GroundUnit{

	Scv(int hp) {
		super(50);
		this.hitpoint = this.MAX_HP;
	}
	
	@Override
	public String toString() {
		return "Scv";
	}
}




